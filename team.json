{
  "version": "1.0",
  "orchestrator": {
    "model": "gpt-4",
    "temperature": 0,
    "max_tokens": 4000
  },
  "workers": [
    {
      "id": "claude-main",
      "agent_type": "claude",
      "capabilities": [
        "complex_logic",
        "architecture",
        "multi_file_changes",
        "deep_analysis",
        "refactoring"
      ],
      "mcp_config": {
        "command": "npx",
        "args": ["-y", "@anthropic-ai/claude-mcp"],
        "env": {
          "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}"
        }
      },
      "max_concurrent_tasks": 3,
      "enabled": false
    },
    {
      "id": "gpt4-main",
      "agent_type": "gpt4",
      "capabilities": [
        "algorithms",
        "performance",
        "testing",
        "debugging",
        "integration"
      ],
      "mcp_config": {
        "command": "python",
        "args": ["workers/openai_worker.py"],
        "env": {
          "OPENAI_API_KEY": "${OPENAI_API_KEY}"
        }
      },
      "max_concurrent_tasks": 2,
      "enabled": false
    },
    {
      "id": "codex-fast",
      "agent_type": "codex",
      "capabilities": [
        "refactoring",
        "type_hints",
        "python",
        "javascript",
        "unit_tests",
        "quick_fixes"
      ],
      "mcp_config": {
        "command": "python",
        "args": ["workers/codex_worker.py"],
        "env": {
          "OPENAI_API_KEY": "${OPENAI_API_KEY}"
        }
      },
      "max_concurrent_tasks": 5,
      "enabled": false
    }
  ],
  "global_forbid": [
    ".git/",
    "venv/",
    ".venv/",
    ".env",
    "node_modules/",
    "dist/",
    "build/",
    "__pycache__/",
    "*.pyc",
    "secrets/",
    ".DS_Store"
  ],
  "quality_gates": {
    "linters": ["ruff", "mypy"],
    "test_runner": "pytest",
    "coverage_threshold": 80
  }
}
